#!/bin/bash
echo "Enter first string:"
read str1
echo "Enter second string:"
read str2
[ "$str1" = "$str2" ] && echo "Strings are equal" || echo "Strings are not equal"
[ -z "$str1" ] || [ -z "$str2" ] && echo "One string is empty" || echo "Both strings are not empty"
echo "Concatenated string: $str1$str2"


# Output:
Enter first string:
hello
Enter second string:
world
Strings are not equal
Both strings are not empty
Concatenated string:Â helloworld
